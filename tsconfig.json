{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "typeRoots": ["./node_modules/@types", "./types"],
    "baseUrl": "./",
    "paths": {
      "@assets/*": ["assets/*"],
      "@components/*": ["components/*"],
      "@config/*": ["config/*"],
      "@context/*": ["context/*"],
      "@functions/*": ["functions/*"],
      "@styles/*": ["styles/*"],
      "@types/*": ["types/*"],
    },
    "jsx": "react-jsx",
    "moduleResolution": "bundler" 

  },
  "include": ["types/env.d.ts", "types/custom_interface_types.ts", "**/*.ts", "**/*.tsx" ],

} 
